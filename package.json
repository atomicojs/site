        "@atomico/tsconfig": "1.0.0",
        "@atomico/vite": "latest",
        "happy-dom": "latest",
        "typescript": "^4.7.4",
        "vite": "latest",
        "vitest": "latest"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "postcss": {
        "plugins": {
            "@atomico/postcss-tokens": {}
        }
    },
    "module": "./dist/components.js",
