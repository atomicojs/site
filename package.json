{
    "name": "@atomico/site",
    "description": "Atomico site Components",
    "workspaces": [
        "src/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "exports": "exports src/**/*.{ts,tsx} !src/site.tsx --exports --types --main components",
        "prepublishOnly": "npm run exports",
        "create": "scaff templates/component src"
    },
    "dependencies": {
        "@atomico/hooks": "^3.45.0",
        "atomico": "latest"
    },
